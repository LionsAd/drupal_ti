<!--?xml version="1.0" encoding="UTF-8"?-->

<phpunit colors="true" bootstrap="./vendor/autoload.php">
  <testsuites>
    <testsuite name="DrupalTiTest">
        <directory>./src/</directory>
    </testsuite>
  </testsuites>
  <!-- Filter for coverage reports. -->
  <filter>
    <blacklist>
      <directory>./vendor</directory>
    </blacklist>
    <whitelist>
      <directory>../src</directory>
    </whitelist>
  </filter>
</phpunit>
